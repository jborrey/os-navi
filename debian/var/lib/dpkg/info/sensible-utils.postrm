-rwxr-xr-x 1 root root 313 Nov 26 2010 var/lib/dpkg/info/sensible-utils.postrm

#! /bin/sh

set -e

case "$1" in
    (remove|disappear)
    if which update-mime >/dev/null;
    then
            update-mime
    fi
        ;;
    (upgrade|failed-upgrade|purge|abort-install|abort-upgrade)
        ;;
    (*)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac
